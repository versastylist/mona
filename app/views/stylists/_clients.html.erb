<h1>Clients</h1>

<table class="table table-hover">
  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone Number</th>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.first_name %></td>
        <td><%= client.last_name %></td>
        <td><%= client.phone_number %></td>
      </tr>
    <% end %>
  </tbody>
</table>


