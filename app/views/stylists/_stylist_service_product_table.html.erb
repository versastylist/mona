<%= bs_row do %>
  <%= bs_column(12) do %>
    <h2 id="<%= product_type.downcase.split.join('_') %>"><%= product_type %></h2>
    <table class="table table-hover">
      <thead>
        <th width="25%">Name</th>
        <th width="50%%">Description</th>
        <th width="10%%">Status</th>
        <th width="10%">Duration</th>
        <th width="5%%">Settings</th>
      </thead>
      <tbody>
        <%= render partial: 'stylists/stylist_service_product', collection: product_collection %>
      </tbody>
    </table>
  <% end %>
<% end %>
