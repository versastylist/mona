<%= bs_column(12) do %>
  <h2><%= product_type %></h2>
  <table class="table table-hover">
    <thead>
      <th>Name</th>
      <th>Price</th>
      <th>Duration (minutes)</th>
      <th>Details</th>
      <th>Quantity</th>
      <th></th>
    </thead>
    <tbody>
      <%= render partial: 'stylists/client_service_product', collection: product_collection %>
    </tbody>
  </table>
<% end %>
